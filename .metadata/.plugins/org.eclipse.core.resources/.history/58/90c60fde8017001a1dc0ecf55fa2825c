package Entities;

public class Rectangle {

	public double width;
	public double height;

	public double area() {

		return this.width * this.height;
	}

	public double perimetro() {
		double p = width + width + height + height;
		return p;
	}

	public double diagonal() {
		double p = 2 * (width + height); // P = 2(b + h)
		return p;
	}

}
